  "scripts": {
    "start": "node build/server-bundle.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --mode development --watch build --exec \"node build/server-bundle.js\"",
    "dev:build-server": "webpack --mode development --config webpack.config.server.js --watch",
    "dev:build-client": "webpack --mode development --config webpack.config.client.js --watch",
    "build": "npm-run-all --parallel prod:*",
    "prod:build-server": "webpack --mode production --config webpack.config.server.js",
    "prod:build-client": "webpack --mode production --config webpack.config.client.js",
    "test": ""
  },
